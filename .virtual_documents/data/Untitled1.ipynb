def my_sq(x):
    '''
    제곱해서 리턴하는 함수
    '''
    return x ** 2


my_sq(2)


def avg_2(x,y):
    '''
    두 숫자의 평균을 구하는 함수
    '''
    return (x+y)/2


avg_2(4,7)


